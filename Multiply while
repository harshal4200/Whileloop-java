public class mul_one_fivebytwo {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the no.: ");
        int i=sc.nextInt();
        int mul=0;
        while (i<=5){
            mul =i*2;
            System.out.println(mul);
            i++;
        }
    }
}
